<h1>All Users</h1>

<% @users.each do |user| %>
<ul>

  <li>Name: <%= user.name %><%= cl_image_tag(user.image, :class => 'icon') %></li>
  <li>Email: <%= user.email %></li>
  <li>Title: <%= user.title %></li>
  <li>Bio: <%= user.bio %></li>
  <li>Created: <%= user.created_at %></li>

</ul>
<% end %>
